pipeline 
{
    agent any
    stages 
    {
        stage('Run Test') 
        {
            steps 
            {
                //sh
                bat "cd GridAndTest"
                bat "docker-compose up --no-color"
            }
        }
        stage('Bring it down') 
        {
            steps 
            {
                //sh
                bat "cd GridAndTest"
                bat "docker-compose down"
            }
        }
        
    }
}