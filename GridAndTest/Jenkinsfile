pipeline 
{
    agent any
    stages 
    {
        stage('Run Test') 
        {
            steps 
            {
                //sh      
                bat "docker-compose -f D:/DockerComposeFiles/GridAndTest/docker-compose.yaml up --no-color"
            }
        }
        stage('Bring it down') 
        {
            steps 
            {
                //sh
                bat "cd GridAndTest"
                bat "docker-compose -f D:/DockerComposeFiles/GridAndTest/docker-compose.yaml down"
            }
        }
        
    }
}